{
    "name": "treggo-portal",
    "productName": "Treggo Portal",
    "version": "0.1.0",
    "description": "An online technical restaurant portal",
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "start": "node server.js",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron-tsc": "tsc main.ts && ng build --base-href ./ && electron .",
        "electron": "ng build --base-href ./ && electron .",
        "electron-build": "ng build --prod && npm run electron .",
        "package-win": "electron-packager . treggo-portal --overwrite --asar --platform=win32 --arch=x64 --icon=build/icon.ico --prune --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Treggo Portal\"",
        "create-installer-win": "node installers/windows/createinstaller.js",
        "heroku-postbuild": "ng build --prod",
        "postinstall": "ng build --prod"
    },
    "private": true,
    "engines": {
        "node": "10.15.3",
        "npm": "6.4.1"
    },
    "dependencies": {
        "@angular/animations": "~8.2.9",
        "@angular/common": "~8.2.9",
        "@angular/compiler": "~8.2.9",
        "@angular/core": "~8.2.9",
        "@angular/forms": "~8.2.9",
        "@angular/platform-browser": "~8.2.9",
        "@angular/platform-browser-dynamic": "~8.2.9",
        "@angular/router": "~8.2.9",
        "angular4-datepicker": "^1.0.1",
        "chart.js": "^2.9.3",
        "express": "^4.17.1",
        "typescript": "~3.5.3",
        "mydatepicker": "^2.6.6",
        "ng2-charts": "^2.3.0",
        "ng2-order-pipe": "^0.1.5",
        "ng2-search-filter": "^0.5.1",
        "ngx-bootstrap": "^5.2.0",
        "ngx-order-pipe": "^2.0.4",
        "ngx-pagination": "^4.1.0",
        "ngx-perfect-scrollbar": "^8.0.0",
        "ngx-print": "^1.2.0-beta.3",
        "rxjs": "~6.4.0",
        "sweetalert2": "^9.3.5",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.24",
        "@angular/cli": "~8.3.9",
        "@angular/compiler-cli": "~8.2.9",
        "@angular/language-service": "~8.2.9",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "^5.0.0",
        "electron": "^11.5.0",
        "electron-packager": "^13.1.1",
        "electron-winstaller": "^4.0.0",
        "enhanced-resolve": "^3.3.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0"
    }
}